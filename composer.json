    "require": {
        "php": ">=7.3",
        "phpunit/phpunit": "^9.0",
        "ext-gd": "*"
    },
    "require-dev": {
        "phpmd/phpmd": "@stable"
    },
    "autoload": {
        "psr-4": {
            "AssertGD\\": "src/"
        }
    }
}
